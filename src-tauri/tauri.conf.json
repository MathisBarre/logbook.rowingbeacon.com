{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Rowing Beacon",
  "mainBinaryName": "Rowing Beacon",
  "version": "1.2.0",
  "identifier": "com.rowing-beacon-tauri-app.app",
  "plugins": {
    "sql": {
      "preload": ["sqlite:mydatabase.db"]
    }
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "alwaysOnTop": false,
        "fullscreen": true,
        "resizable": true,
        "maximizable": true,
        "maximized": true,
        "title": "RowingBeacon",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
